# =============================================================================
# STORAGE SERVICES CONFIGURATION (docker-compose.storage.yml)
# =============================================================================

# MinIO Object Storage
MINIO_ROOT_USER=your_minio_user
MINIO_ROOT_PASSWORD=your_minio_password

# PostgreSQL Database
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password

# PgAdmin
PGADMIN_DEFAULT_EMAIL=your_email@example.com
PGADMIN_DEFAULT_PASSWORD=your_pgadmin_password

# Milvus MinIO (separate from books MinIO)
MILVUS_MINIO_ACCESS_KEY=your_milvus_minio_access_key
MILVUS_MINIO_SECRET_KEY=your_milvus_minio_secret_key

# Milvus root password
COMMON_SECURITY_DEFAULTROOTPASSWORD=your_default_milvus_root_password